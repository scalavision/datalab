{
  "files.exclude": {
    "**/*.keep": true,
    "target/": true,
    "**/.bsp": true,
    "**/.metals": true,
    "**/.sbt": true,
    "**/.bloop": true,
    "project/target": true,
    "project/project": true,
    "node_modules": true,
    "**/*.pyc": {
      "when": "$(basename).py"
    },
    "**/__pycache__": true
  },
  "files.watcherExclude": {
    "**/target": true,
    "**/.bsp": true,
    "**/.metals": true,
    "**/.sbt": true,
    "**/.bloop": true
  },
  "nixEnvSelector.nixFile": "${workspaceRoot}/bioanalytics.nix",
  "python.formatting.provider": "black"
}
